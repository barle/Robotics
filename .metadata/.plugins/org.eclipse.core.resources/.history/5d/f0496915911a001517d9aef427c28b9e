#include "Vertex.h"


void Vertex::setH(int h)
{
	_h = h;
}

void Vertex::setG(int g)
{
	_g = g;
}

int Vertex::getH()
{
	return _h;
}

int Vertex::getG()
{
	return _g;
}

int Vertex::getF()
{
	return _h + _g;
}

vector<Vertex> Vertex::getAdj()
{
	return _adj;
}

Vertex::Vertex() {
	// TODO Auto-generated constructor stub

}

Vertex::~Vertex() {
	// TODO Auto-generated destructor stub
}

