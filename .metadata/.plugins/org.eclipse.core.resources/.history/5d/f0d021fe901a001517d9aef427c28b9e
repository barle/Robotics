#ifndef VERTEX_H_
#define VERTEX_H_

#include <vector>

using namespace std;

class Vertex {
private:
	int _h;
	int _g;
	Position _location;
	vector<Vertex> _adj;
public:
	void setH(int h);
	void setG(int g);

	int getF();
	int getH();
	int getG();

	Position getLocation();

	Vertex(Position location);
	virtual ~Vertex();
};

#endif /* VERTEX_H_ */
