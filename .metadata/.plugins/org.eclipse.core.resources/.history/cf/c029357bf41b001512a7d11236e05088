#include "GoTo.h"

GoTo::GoTo(Robot *robot, Position *goal) : Behavior(robot) {
	this->_goal = goal;
	this->_achived = false;
}

void GoTo::action()
{

}

bool GoTo::startCond()
{
	return !this->_achived;
}

bool GoTo::stopCond()
{
	return this->_achived;
}

GoTo::~GoTo() {
	// TODO Auto-generated destructor stub
}

